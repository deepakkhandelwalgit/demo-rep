{
  "name": "react-npm-skeleton-demo",
  "version": "1.0.0",
  "description": "A skeleton project for creating react components packaged with npm",
  "main": "index.js",
  "scripts": {
    "prepublish": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepakkhandelwalgit/demo-rep.git"
  },
  "keywords": [
    "react-component"
  ],
  "author": "Cameron Stuart",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deepakkhandelwalgit/demo-rep/issues"
  },
  "homepage": "https://github.com/deepakkhandelwalgit/demo-rep#readme",
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-istanbul": "^0.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "coveralls": "^2.11.9",
    "expect": "^1.16.0",
    "expect-jsx": "^2.4.0",
    "istanbul": "^0.4.2",
    "jquery": "^2.2.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-bootstrap": "^0.28.5",
    "react-dom": "^0.14.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "eslint-plugin-react": "^4.2.3"
  }
}
